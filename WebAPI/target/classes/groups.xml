<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="group">

    <test name="run-server">
        <groups>
            <run>
                <include name="server"></include>
            </run>
        </groups>
        <classes>
            <class name="com.webapi.testng.groups.GroupsOnMethod"></class>
        </classes>
    </test>

    <test name="run-client">
        <groups>
            <run>
                <include name="client"></include>
            </run>
        </groups>

        <classes>
            <class name="com.webapi.testng.groups.GroupsOnMethod"></class>
        </classes>
    </test>

</suite>